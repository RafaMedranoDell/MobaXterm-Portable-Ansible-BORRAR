# Install this package as a python alternative.
# PYTHON_VERSION=3.7
# PYTHON_PRIORITY=37
/usr/sbin/alternatives --install /usr/bin/python python /usr/bin/python3.7 37
/usr/sbin/alternatives --install /usr/bin/python3 python3 /usr/bin/python3.7 37
/usr/sbin/alternatives --install /usr/bin/pydoc3 pydoc3 /usr/bin/pydoc3.7 37

rm -f /usr/share/icons/hicolor/icon-theme.cache
